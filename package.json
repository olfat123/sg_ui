		"url": "https://github.com/mohamedashrafothman"
	},
	"description": "TCG Website",
	"private": true,
	"license": "ISC",
	"main": "gulpfile.js",
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=7.0.0"
	},
	"homepage": "https://github.com/mohamedashrafothman/tcg-ui#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mohamedashrafothman/tcg-ui.git"
	},
	"bugs": {
		"url": "https://github.com/mohamedashrafothman/tcg-ui/issues"
	},
	"scripts": {
		"clean": "rimraf ./build",
		"prestart": "npm run clean",
		"start": "cross-env NODE_OPTIONS='--inspect' gulp",
		"prebuild": "npm run clean",
		"build": "cross-env NODE_ENV=production gulp production --production",
		"prepare": "husky install",
		"lint": "eslint ./src/**/*.js",
		"prettify": "prettier --write --ignore-path .gitignore",
		"deploy": "sudo chmod +x ./deploy.sh && sudo ./deploy.sh",
		"get-next-release-version": "node ./get-next-release-version.js",
		"release": "standard-version",
		"release:major": "standard-version --release-as major",
		"release:minor": "standard-version --release-as minor",
		"release:patch": "standard-version --release-as patch"
	},
	"devDependencies": {
		"@babel/core": "7.21.4",
